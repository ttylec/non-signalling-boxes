       RowBox[{"{", "}"}]}], " ", "&&", " ", 
      RowBox[{"Not", "[", 
       RowBox[{"isLess", "[", 
        RowBox[{"p", ",", 
         RowBox[{"orthocmpl", "[", "q", "]"}]}], "]"}], "]"}]}], ",", " ", 
