  RowBox[{"mutuallyOrthogonal", "[", 
   RowBox[{"p_", ",", "q_", ",", "r_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"isLess", "[", 
    RowBox[{"p", ",", 
     RowBox[{"orthocmpl", "[", "r", "]"}]}], "]"}], "&&", 
   RowBox[{"isLess", "[", 
    RowBox[{"q", ",", 
     RowBox[{"orthocmpl", "[", "r", "]"}]}], "]"}], "&&", 
   RowBox[{"isLess", "[", 
    RowBox[{"p", ",", 
     RowBox[{"orthocmpl", "[", "q", "]"}]}], "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
