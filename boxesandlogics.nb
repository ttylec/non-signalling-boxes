    RowBox[{"question", "[", "1", "]"}], "]"}], ":=", "\"\<1\>\""}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"printQ", "[", "q_question", "]"}], ":=", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{
     RowBox[{"\"\<[\>\"", "<>", 
      RowBox[{"list2string", "[", 
       RowBox[{
        RowBox[{"#", "[", 
         RowBox[{"[", "1", "]"}], "]"}], "-", "1"}], "]"}], "<>", "\"\<,\>\"",
       "<>", 
      RowBox[{"list2string", "[", 
       RowBox[{
        RowBox[{"#", "[", 
         RowBox[{"[", "2", "]"}], "]"}], "-", "1"}], "]"}], "<>", 
